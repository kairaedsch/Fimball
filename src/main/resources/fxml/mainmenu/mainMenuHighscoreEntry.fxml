<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>

<GridPane styleClass="highscoreEntry" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sep.fimball.view.general.HighscoreSubView">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="1.0" prefHeight="1.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <Label GridPane.columnIndex="0" GridPane.rowIndex="0" fx:id="playerName" maxHeight="9999" styleClass="highscoreEntryName" text="Till"/>
    <Label GridPane.columnIndex="1" GridPane.rowIndex="0" fx:id="score" minWidth="80.0" styleClass="highscoreEntryScore" text="10000"/>
    <Pane GridPane.columnIndex="0" GridPane.rowIndex="1" minHeight="1.0" minWidth="80.0" styleClass="highscoreEntryLine"/>
    <Pane GridPane.columnIndex="1" GridPane.rowIndex="1" minHeight="1.0" minWidth="80.0" styleClass="highscoreEntryLine"/>
</GridPane>