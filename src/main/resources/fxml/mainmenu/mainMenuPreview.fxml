<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import java.lang.String?>
<HBox styleClass="machine, selected" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sep.fimball.view.window.mainmenu.PinballMachineSelectorSubView" onMouseClicked="#mouseClicked">
    <children>
        <Pane minHeight="80.0" minWidth="80.0" styleClass="machinePic" fx:id="previewImage" />
        <Label maxHeight="9999" text="Marcs Automat" styleClass="machineName"  fx:id="previewName" ></Label>
    </children>
    <stylesheets>
        <String fx:value="styles/style.css"/>
    </stylesheets>
</HBox>
