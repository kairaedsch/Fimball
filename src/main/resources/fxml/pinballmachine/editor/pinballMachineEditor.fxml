<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@../../../styles/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sep.fimball.view.window.pinballmachine.editor.PinballMachineEditorView">
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <SplitPane dividerPositions="0.21517996870109546, 0.8176838810641628" VBox.vgrow="ALWAYS" styleClass="splitPane">
            <AnchorPane minWidth="290.0">
                <VBox styleClass="dialogBackground" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <HBox alignment="CENTER_LEFT" spacing="10.0" styleClass="darkBackground">
                        <VBox.margin>
                            <Insets/>
                        </VBox.margin>
                        <padding>
                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                        </padding>
                        <VBox>
                            <Button fx:id="settings" maxWidth="120.0" minWidth="120.0" mnemonicParsing="false" onMouseClicked="#settingsClicked"
                                    styleClass="dialogButton" text="!editor.exit.key!">
                                <VBox.margin>
                                    <Insets bottom="5.0"/>
                                </VBox.margin>
                            </Button>
                            <Button fx:id="abort" maxWidth="120.0" minWidth="120.0" mnemonicParsing="false" onMouseClicked="#abortClicked"
                                    styleClass="dialogButton" text="!editor.abort.key!"/>
                        </VBox>
                        <Label fx:id="nameLabel" text="Tills Automat" textFill="WHITE">
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Label>
                    </HBox>
                    <Accordion VBox.vgrow="ALWAYS">
                        <panes>
                            <TitledPane animated="false" text="Basic Elements" styleClass="availableElementsTitle">
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <ScrollPane styleClass="edge-to-edge" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" vbarPolicy="ALWAYS"
                                                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                                AnchorPane.topAnchor="0.0">
                                        <AnchorPane styleClass="availableElementsList">
                                            <VBox fx:id="availableElementsBasic" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            </VBox>
                                        </AnchorPane>
                                    </ScrollPane>
                                </AnchorPane>
                            </TitledPane>
                            <TitledPane animated="false" text="Obstacles" styleClass="availableElementsTitle">
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <ScrollPane styleClass="edge-to-edge" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" vbarPolicy="ALWAYS"
                                                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                                AnchorPane.topAnchor="0.0">
                                        <AnchorPane styleClass="availableElementsList">
                                            <VBox fx:id="availableElementsObstacles" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            </VBox>
                                        </AnchorPane>
                                    </ScrollPane>
                                </AnchorPane>
                            </TitledPane>
                            <TitledPane animated="false" text="Ramps" styleClass="availableElementsTitle">
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <ScrollPane styleClass="edge-to-edge" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" vbarPolicy="ALWAYS"
                                                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                                AnchorPane.topAnchor="0.0">
                                        <AnchorPane styleClass="availableElementsList">
                                            <VBox fx:id="availableElementsRamp" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            </VBox>
                                        </AnchorPane>
                                    </ScrollPane>
                                </AnchorPane>
                            </TitledPane>
                            <TitledPane animated="false" text="Advanced Elements" styleClass="availableElementsTitle">
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <ScrollPane styleClass="edge-to-edge" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" vbarPolicy="ALWAYS"
                                                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                                AnchorPane.topAnchor="0.0">
                                        <AnchorPane styleClass="availableElementsList">
                                            <VBox fx:id="availableElementsAdvanced" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            </VBox>
                                        </AnchorPane>
                                    </ScrollPane>
                                </AnchorPane>
                            </TitledPane>
                        </panes>
                    </Accordion>
                </VBox>
            </AnchorPane>
            <AnchorPane>
                <StackPane fx:id="pinballCanvasContainer" onMouseDragEntered="#mouseDragEnteredCanvas" onMouseDragExited="#mouseDragExitedCanvas"
                           onMouseDragOver="#draggedOver" onMouseDragged="#dragged" onMouseEntered="#mouseEnteredCanvas" onMouseExited="#mouseExitedCanvas"
                           onMousePressed="#down" onMouseReleased="#mouseReleased" onScroll="#zoom" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <padding>
                        <Insets left="5.0" top="5.0"/>
                    </padding>
                    <Button mnemonicParsing="false" onMouseClicked="#playClicked" styleClass="dialogButton" text="Play">
                        <HBox.margin>
                            <Insets right="10.0"/>
                        </HBox.margin>
                    </Button>
                    <Button maxHeight="25.0" maxWidth="25.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onMouseClicked="#zoomInClicked"
                            styleClass="dialogButton" text="+"/>
                    <Button maxHeight="25.0" maxWidth="25.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onMouseClicked="#zoomOutClicked"
                            styleClass="dialogButton" text="-">
                        <HBox.margin>
                            <Insets left="3.0"/>
                        </HBox.margin>
                    </Button>
                </HBox>
            </AnchorPane>
            <AnchorPane minWidth="290.0">
                <TitledPane fx:id="selectedElement" animated="false" collapsible="false" text="Bumper Properties" textFill="WHITE" AnchorPane.bottomAnchor="0.0"
                            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <styleClass>
                        <String fx:value="titled-pane-editor-props"/>
                        <String fx:value="dialogBackground"/>
                    </styleClass>
                </TitledPane>
            </AnchorPane>
        </SplitPane>
    </VBox>
    <Pane fx:id="previewBot" mouseTransparent="true" prefHeight="50.0" prefWidth="50.0">
        <Pane fx:id="previewTop" mouseTransparent="true" prefHeight="50.0" prefWidth="50.0"/>
    </Pane>
</AnchorPane>
