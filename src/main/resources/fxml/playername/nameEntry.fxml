<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" xmlns="http://javafx.com/javafx/8" fx:controller="">
    <columnConstraints>
        <ColumnConstraints minWidth="10.0" prefWidth="150.0"/>
        <ColumnConstraints minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <TextField fx:id="nameTextField" text="Player" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Button mnemonicParsing="false" text="X" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    </children>
</GridPane>
