<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<TitledPane fx:id="key_0" collapsible="false" maxWidth="300.0" text="!playername.title.key!" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sep.fimball.view.dialog.playername.PlayerNameView" stylesheets="@../../styles/style.css" >
    <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="dialogBackground, playerName">
        <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="10.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Label fx:id="key_1" text="!playername.names.key!">
                <font>
                    <Font name="System Bold" size="14.0"/>
                </font>
                <VBox.margin>
                    <Insets/>
                </VBox.margin>
            </Label>
            <VBox fx:id="nameEntryList" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
            <Button fx:id="addPlayerButton" mnemonicParsing="false" onAction="#addPlayerClicked" text="!playername.add.key!" styleClass="dialogButton"/>
        </VBox>
        <Button fx:id="startButton" defaultButton="true" mnemonicParsing="false" onMouseClicked="#okClicked" text="!playername.continue.key!" AnchorPane.bottomAnchor="0.0"
                AnchorPane.rightAnchor="0.0" styleClass="dialogButton"/>
        <Button fx:id="key_4"  mnemonicParsing="false" onAction="#abortClicked" text="!playername.exit.key!" AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0" styleClass="dialogButton"/>
    </AnchorPane>
